        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^29.2.1",
        "prettier": "^2.5.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
